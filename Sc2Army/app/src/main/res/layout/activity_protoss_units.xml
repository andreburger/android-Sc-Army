<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="scarmy.za.ac.cput.ScArmyBuilder.ProtossUnits"
    android:background="#673AB7">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Protoss Units"
        android:id="@+id/textView21"
        android:textColor="#FFFFFF"
        android:textSize="40dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current Pop Cap:"
        android:id="@+id/textView28"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignTop="@+id/textView26"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/txtpopP"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignTop="@+id/textView26"
        android:layout_alignRight="@+id/textView21"
        android:layout_alignEnd="@+id/textView21" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/200"
        android:id="@+id/textView26"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_below="@+id/textView21"
        android:layout_toRightOf="@+id/textView21"
        android:layout_toEndOf="@+id/textView21"
        android:layout_marginTop="43dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zealot"
        android:id="@+id/btnZealot"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_below="@+id/textView28"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="39dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Stalker"
        android:id="@+id/btnStalker"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_below="@+id/btnZealot"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pheonix"
        android:id="@+id/btnPheonix"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_below="@+id/btnStalker"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="High Templar"
        android:id="@+id/btnHT"
        android:textColor="#FFFFFF"
        android:background="#673AB7"
        android:textSize="20dp"
        android:layout_below="@+id/btnPheonix"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dark Templar"
        android:id="@+id/btnDT"
        android:textColor="#FFFFFF"
        android:background="#673AB7"
        android:textSize="20dp"
        android:layout_below="@+id/btnHT"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel"
        android:id="@+id/btnCancelP"
        android:textColor="#FFFFFF"
        android:background="#673AB7"
        android:textSize="20dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Void Ray"
        android:id="@+id/btnVoidray"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:background="#673AB7"
        android:layout_alignTop="@+id/btnZealot"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sentry"
        android:id="@+id/btnSentry"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_below="@+id/btnVoidray"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Immortal"
        android:id="@+id/btnImmortal"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_above="@+id/btnHT"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Carrier"
        android:id="@+id/btnCarrier"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignTop="@+id/btnHT"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Colossus"
        android:id="@+id/btnColossus"
        android:textColor="#FFFFFF"
        android:background="#673AB7"
        android:textSize="20dp"
        android:layout_alignTop="@+id/btnDT"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Create"
        android:id="@+id/btnCreateP"
        android:textColor="#FFFFFF"
        android:background="#673AB7"
        android:textSize="20dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Archon"
        android:id="@+id/btnArchon"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignTop="@+id/btnVoidray"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
