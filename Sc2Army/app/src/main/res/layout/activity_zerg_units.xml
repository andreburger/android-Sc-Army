<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="scarmy.za.ac.cput.ScArmyBuilder.ZergUnits"
    android:background="#673AB7">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zerg Units"
        android:id="@+id/textView23"
        android:textColor="#FFFFFF"
        android:textSize="40dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:allowUndo="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current Pop Cap:"
        android:id="@+id/textView29"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_marginTop="41dp"
        android:layout_below="@+id/textView23"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/200"
        android:id="@+id/textView30"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignTop="@+id/textView29"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/txtpopZ"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignTop="@+id/textView30"
        android:layout_toLeftOf="@+id/textView30"
        android:layout_toStartOf="@+id/textView30" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Baneling"
        android:id="@+id/btnBaneling"
        android:layout_marginTop="46dp"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_below="@+id/textView29"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Corruptor"
        android:id="@+id/btnCorruptor"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_below="@+id/btnBaneling"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Infestor"
        android:id="@+id/btnInfestor"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_below="@+id/btnCorruptor"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Queen"
        android:id="@+id/btnQueen"
        android:textColor="#FFFFFF"
        android:background="#673AB7"
        android:textSize="20dp"
        android:layout_below="@+id/btnInfestor"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ultralisk"
        android:id="@+id/btnUltralisk"
        android:textColor="#FFFFFF"
        android:background="#673AB7"
        android:textSize="20dp"
        android:layout_below="@+id/btnQueen"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cancel"
        android:id="@+id/btnCancelZ"
        android:textColor="#FFFFFF"
        android:background="#673AB7"
        android:textSize="20dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Create"
        android:id="@+id/btnCreateZ"
        android:textColor="#FFFFFF"
        android:background="#673AB7"
        android:textSize="20dp"
        android:layout_alignTop="@+id/btnCancelZ"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Broodlord"
        android:id="@+id/btnBroodlord"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:background="#673AB7"
        android:layout_alignTop="@+id/btnBaneling"
        android:layout_alignRight="@+id/textView30"
        android:layout_alignEnd="@+id/textView30" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hydralisk"
        android:id="@+id/btnHydralisk"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_below="@+id/btnBroodlord"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mutalisk"
        android:id="@+id/btnMutalisk"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_below="@+id/btnHydralisk"
        android:layout_alignRight="@+id/btnHydralisk"
        android:layout_alignEnd="@+id/btnHydralisk" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Roach"
        android:id="@+id/btnRoach"
        android:background="#673AB7"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:layout_alignTop="@+id/btnQueen"
        android:layout_alignRight="@+id/btnMutalisk"
        android:layout_alignEnd="@+id/btnMutalisk" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zergling"
        android:id="@+id/btnZergling"
        android:textColor="#FFFFFF"
        android:background="#673AB7"
        android:textSize="20dp"
        android:layout_below="@+id/btnRoach"
        android:layout_alignRight="@+id/btnCreateZ"
        android:layout_alignEnd="@+id/btnCreateZ" />
</RelativeLayout>
